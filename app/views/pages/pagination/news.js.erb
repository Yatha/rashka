var news = $("#news_pagination"),
  overview = news.find(".overview");
news.find(".pagination").remove();

if (overview.length > 0) {
  overview.append("<%= j render :partial => 'pages/news_pagination' %>");
} else {
  news.append("<%= j render :partial => 'pages/news_pagination' %>");
}
news.customScrollbar("resize", true);